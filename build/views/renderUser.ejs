<!DOCTYPE html>
<html>
<head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>

<h2>HTML Table</h2>

<table>
  <tr>
    <th>Email</th>
    <th>lastName</th>
    <th>password</th>
  </tr>
  <% for(let i = 0; i < dataTable.length;i++) { %>
  <tr>
    <td> <%= dataTable[i].email %></td>
    <td><%= dataTable[i].lastName %></td>
    <td><%= dataTable[i].password %></td>
    <td>
      <a href="http://localhost:8080/edit?id=<%= dataTable[i].id %>" >Edit</a>
      <a href="http://localhost:8080/delete?id=<%= dataTable[i].id %>" >Delete</a>
    </td>
  </tr>
  <% } %>
</table>

</body>
</html>
